        <div class="container">
            <div class="row">
                <hr class="azul">
                <div class="col-md-12">
                    <ul class="media-list">
                        <li class="media">
                            <a class="pull-left" href="/"><img class="img-circle media-object" src="logolarge.jpg" height="120" width="120"></a>
                            <div class="media-body">
                                <h2 class="text-center">{{titulo}}</h2>
                            </div>
                        </li>
                        <li class="media"></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <hr class="blanco">
