        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="thumbnail">
                        <img src="error.png" class="img-responsive" height="120" width="120">
                        <div class="caption">
                            <h1 class="text-center">{{error}}</h1>
                            <div class="col-md-12 text-center">
                                <a href="/" class="btn btn-primary" role="button">Regresar Página Principal</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-12">
        <h1 class="text-center"></h1>
    </div>
    <div class="section">
